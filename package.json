    "prettier": "prettier --config .prettierrc \"**/*.ts\" --write",
    "esbuild": "node build.mjs",
    "build": "rimraf dist && npm run lint:ts && npm run esbuild",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --config jest.config.mjs"
  },
  "author": "MikletNg<iam@mikeng.io>",
  "license": "MIT",
  "devDependencies": {
    "@esbuild-plugins/node-globals-polyfill": "^0.2.3",
    "@esbuild-plugins/node-modules-polyfill": "^0.2.2",
    "@eslint/js": "^9.21.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.8",
    "@typescript-eslint/eslint-plugin": "^8.25.0",
    "@typescript-eslint/parser": "^8.25.0",
    "dotenv": "^16.4.7",
    "esbuild": "^0.25.0",
    "eslint": "^9.22.0",
    "eslint-plugin-jest": "^28.11.0",
    "jest": "^29.7.0",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.2.6",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.3",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.25.0"
  },
  "dependencies": {
    "@aws-sdk/client-kms": "^3.758.0",
    "@peculiar/asn1-ecc": "^2.3.15",
    "@peculiar/asn1-schema": "^2.3.15",
    "asn1js": "^3.0.5",
    "ethers": "^6.13.5",
    "lodash": "^4.17.21",
    "pkijs": "^3.2.4",
    "tronweb": "^6.0.1",
    "winston": "^3.17.0"
  },
  "engines": {
    "node": ">=20"
  },
  "exports": {
    ".": {
      "import": {
        "node": "./dist/node/bundle.mjs",
        "browser": "./dist/browser/bundle.mjs",
        "default": "./dist/browser/bundle.mjs"
      },
      "require": {
        "node": "./dist/node/bundle.cjs",
        "browser": "./dist/browser/bundle.js",
        "default": "./dist/node/bundle.cjs"
      }
    }
